<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/bikeStationsList">
    <fragment
        android:id="@+id/bikeStationsList"
        android:name="com.example.bikes.ui.bikes.BikeStationsListFragment"
        tools:layout="@layout/fragment_bike_stations_list">
        <action
            android:id="@+id/action_map"
            app:destination="@id/mapFragment" />

    </fragment>

    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.bikes.ui.map.MapFragment"
        tools:layout="@layout/fragment_map">
        <argument
            android:name="bikeStation"
            app:argType="com.example.bikes.model.domain.BikeStationModel" />
    </fragment>

</navigation>